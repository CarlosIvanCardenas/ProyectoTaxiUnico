@model List<TaxiUnicoWebClient.Models.Classes.Administrador>
@{
    ViewData["Title"] = "GetAllAdmins";
}
<div class="container">
    <div class="page-header clearfix detailHeading">
        <h2>@ViewData["Title"]</h2>
    </div>
</div>

<div class="container">
    <div class="jumbotron" style="overflow:auto;">
        <h3>Clientes: </h3>
        <br>
        <table class="table table-responsive table-striped">
            <thead>
                <tr>
                    <th>PrimerNombre</th>
                    <th>SegundoNombre</th>
                    <th>Apellido</th>
                    <th>Correo</th>
                    <th>Telefono</th>
                    <th>Detalles</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var c in Model)
                {
                    <tr class="assetRow">
                        <td>@c.PrimerNombre</td>
                        <td>@c.SegundoNombre</td>
                        <td>@c.Apellidos</td>
                        <td>@c.Correo</td>
                        <td>@c.Telefono</td>
                        <td><a asp-controller="Admins" asp-action="GetById" asp-route-id="@c.Id" type="button" class="btn btn-sm btn-primary">Detalles</a></td>
                        
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>